<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    <record id="view_users_group_form" model="ir.ui.view">
        <field name="name">res.users.group.form</field>
        <field name="model">res.users</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <button name="%(action_user_group_all)d" string="Group Report" type="action" class="oe_highlight"/>
            </xpath>
        </field>
    </record>
    
    </data>
</openerp>
