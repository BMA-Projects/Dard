<?xml version="1.0" encoding="UTF-8"?>

<openerp>
	<data>

		<template id="website.layout_footer_copyright_inherited"
			inherit_id="website.layout" name="Footer Copyright inherited">
			<xpath expr="//div[@class='container mt16 mb8']" position="replace">
				<div class="container mt16 mb8">
					<div class="pull-right" t-ignore="true" t-if="not editable">
					</div>
					<div class="pull-left text-muted">
						Copyright &amp;copy;
						<span t-field="res_company.name">Company name</span>
					</div>
				</div>
			</xpath>
		</template>
	</data>

</openerp>