      <blockTable colWidths="536" style="Table5">
        <tr>
          <td>
            <para style="terp_default_11">[[ format(line.name) ]] </para>
            <para style="terp_default_9">Imprint Method :[[ line.imprint_method and line.imprint_method.name or removeParentNode('para') ]]</para>
            <para style="terp_default_9">[[ get_imprint_data(line) ]]</para>
            <para style="terp_default_9">PMS Code :[[ ', '.join(map(lambda x: x.name, line.pms_code or removeParentNode('para'))) ]]</para>
          </td>
        </tr>
